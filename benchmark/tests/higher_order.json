{
  "category": "higher_order",
  "description": "Higher-order functional patterns",
  "tests": [
    {
      "name": "fold_sum",
      "file": "examples/higher-order/fold_sum.goth",
      "description": "Sum via fold/recursion",
      "cases": [
        {"input": [0], "expected": 0},
        {"input": [10], "expected": 55},
        {"input": [100], "expected": 5050}
      ]
    },
    {
      "name": "fold_product",
      "file": "examples/higher-order/fold_product.goth",
      "description": "Product via fold (factorial)",
      "cases": [
        {"input": [1], "expected": 1},
        {"input": [5], "expected": 120},
        {"input": [7], "expected": 5040}
      ]
    },
    {
      "name": "compose",
      "file": "examples/higher-order/compose.goth",
      "description": "Function composition square(double(x))",
      "cases": [
        {"input": [0], "expected": 0},
        {"input": [3], "expected": 36},
        {"input": [5], "expected": 100}
      ]
    },
    {
      "name": "apply_twice",
      "file": "examples/higher-order/apply_twice.goth",
      "description": "Apply function twice double(double(x))",
      "cases": [
        {"input": [0], "expected": 0},
        {"input": [3], "expected": 12},
        {"input": [5], "expected": 20}
      ]
    },
    {
      "name": "count_if",
      "file": "examples/higher-order/count_if.goth",
      "description": "Count even numbers in 1..n",
      "cases": [
        {"input": [10], "expected": 5},
        {"input": [7], "expected": 3},
        {"input": [1], "expected": 0}
      ]
    },
    {
      "name": "pipeline",
      "file": "examples/higher-order/pipeline.goth",
      "description": "Sum of squares of evens in 1..n",
      "cases": [
        {"input": [6], "expected": 56},
        {"input": [10], "expected": 220},
        {"input": [4], "expected": 20}
      ]
    }
  ]
}
