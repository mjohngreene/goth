{
  "category": "recursion",
  "description": "Recursive functions with mathematical properties",
  "tests": [
    {
      "name": "factorial",
      "file": "examples/recursion/factorial.goth",
      "description": "Compute n!",
      "cases": [
        {"input": [0], "expected": 1},
        {"input": [1], "expected": 1},
        {"input": [5], "expected": 120},
        {"input": [10], "expected": 3628800}
      ]
    },
    {
      "name": "fibonacci",
      "file": "examples/recursion/fibonacci.goth",
      "description": "Compute F(n)",
      "cases": [
        {"input": [0], "expected": 0},
        {"input": [1], "expected": 1},
        {"input": [10], "expected": 55},
        {"input": [15], "expected": 610}
      ]
    },
    {
      "name": "sum_to_n",
      "file": "examples/recursion/sum_to_n.goth",
      "description": "Sum 1 + 2 + ... + n",
      "cases": [
        {"input": [0], "expected": 0},
        {"input": [1], "expected": 1},
        {"input": [10], "expected": 55},
        {"input": [100], "expected": 5050}
      ]
    },
    {
      "name": "power",
      "file": "examples/recursion/power.goth",
      "description": "Compute base^exp",
      "cases": [
        {"input": [2, 0], "expected": 1},
        {"input": [2, 10], "expected": 1024},
        {"input": [3, 4], "expected": 81}
      ]
    },
    {
      "name": "gcd",
      "file": "examples/recursion/gcd.goth",
      "description": "Greatest common divisor",
      "cases": [
        {"input": [48, 18], "expected": 6},
        {"input": [17, 13], "expected": 1},
        {"input": [100, 25], "expected": 25}
      ]
    },
    {
      "name": "lcm",
      "file": "examples/recursion/lcm.goth",
      "description": "Least common multiple",
      "cases": [
        {"input": [4, 6], "expected": 12},
        {"input": [3, 7], "expected": 21},
        {"input": [12, 18], "expected": 36}
      ]
    },
    {
      "name": "ackermann",
      "file": "examples/recursion/ackermann.goth",
      "description": "Ackermann function A(m,n)",
      "cases": [
        {"input": [0, 0], "expected": 1},
        {"input": [1, 1], "expected": 3},
        {"input": [2, 2], "expected": 7},
        {"input": [3, 4], "expected": 125}
      ]
    },
    {
      "name": "mccarthy91",
      "file": "examples/recursion/mccarthy91.goth",
      "description": "McCarthy 91 function",
      "cases": [
        {"input": [99], "expected": 91},
        {"input": [100], "expected": 91},
        {"input": [101], "expected": 91},
        {"input": [105], "expected": 95}
      ]
    },
    {
      "name": "digit_sum",
      "file": "examples/recursion/digit_sum.goth",
      "description": "Sum of digits",
      "cases": [
        {"input": [0], "expected": 0},
        {"input": [123], "expected": 6},
        {"input": [9999], "expected": 36}
      ]
    },
    {
      "name": "tak",
      "file": "examples/recursion/tak.goth",
      "description": "Takeuchi function",
      "cases": [
        {"input": [6, 4, 2], "expected": 3},
        {"input": [12, 8, 4], "expected": 5},
        {"input": [18, 12, 6], "expected": 7}
      ]
    }
  ]
}
